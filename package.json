{
  "name": "@monorepo/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:client": "nx serve client",
    "build:client": "nx build client",
    "test:client": "nx e2e client-e2e",
    "lint:client": "nx lint client",
    "start:server": "cd apps/server && ./mvnw spring-boot:run",
    "test:server": "cd apps/server && ./mvnw test",
    "test:server:dto": "cd apps/server && ./mvnw test -Dtest=\"DtoRequestTests/**\" -Dsurefire.failIfNoSpecifiedTests=false",
    "test:server:entity": "cd apps/server && ./mvnw test -Dtest=\"com.example.demo.entity.*Test*\" -Dsurefire.failIfNoSpecifiedTests=false",
    "test:server:rest": "cd apps/server && ./mvnw test -Dtest=\"com.example.demo.rest.*Test*\" -Dsurefire.failIfNoSpecifiedTests=false",
    "test:server:unit": "cd apps/server && ./mvnw test -Dtest=\"com.example.demo.dto.*Test*\" -Dsurefire.failIfNoSpecifiedTests=false",
    "test:server:coverage": "cd apps/server && ./mvnw clean test jacoco:report",
    "test:server:quick": "npm run test:server:dto && npm run test:server:entity && npm run test:server:unit",
    "build:server": "cd apps/server && ./mvnw clean compile",
    "package:server": "cd apps/server && ./mvnw clean package",
    "test:all": "concurrently \"npm run test:client\" \"npm run test:server\"",
    "start:all": "concurrently \"npm run start:server\" \"npm run start:client\""
  },
  "private": true,
  "dependencies": {
